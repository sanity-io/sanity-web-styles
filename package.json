{
  "name": "sanity-web-styles",
  "version": "1.0.0-alpha.2",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "repository": "https://github.com/sanity-io/sanity-web-styles",
  "homepage": "https://github.com/sanity-io/sanity-web-styles#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-web-styles/issues"
  },
  "keywords": [
    "sanity.io",
    "web",
    "styles",
    "css"
  ],
  "main": "package.json",
  "style": "dist/sanity.css",
  "scripts": {
    "build": "npm run clean && npm run build-postcss && npm run build-images",
    "build-images": "cpx \"src/**/*.{ico,png}\" dist/",
    "build-postcss": "postcss src/index.css --output dist/sanity.css",
    "clean": "rimraf .cache dist",
    "dev": "npm run watch-postcss",
    "prepublishOnly": "npm run build",
    "watch-html": "parcel src/index.html",
    "watch-postcss": "postcss src/index.css --output dist/sanity.css --watch"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "parcel-bundler": "^1.11.0",
    "postcss-cli": "^6.1.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.5.0",
    "rimraf": "^2.6.3"
  }
}
